<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/cardViewWatchedStyle">

    <androidx.constraintlayout.widget.ConstraintLayout
        style="@style/constraintOfCardViewStyle">

        <androidx.cardview.widget.CardView
            android:id="@+id/watched_fragment_recycler_item_image_view_card_view"
            style="@style/imageCardViewCardViewWatchedStyle"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/watched_fragment_recycler_item_image_view"
                style="@style/imageCardViewWatchedStyle"
                tools:ignore="ContentDescription" />
        </androidx.cardview.widget.CardView>

        <ImageView
            android:id="@+id/attention"
            style="@style/attentionIconCardViewWantedStyle"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/watchedFragmentRecyclerItemTextView"
            style="@style/titleCardViewWatchedStyle"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/item_data"
            style="@style/dataCardViewUnwantedStyle"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>