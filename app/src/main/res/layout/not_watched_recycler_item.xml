<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/cardViewNotWatchedStyle">

    <androidx.constraintlayout.widget.ConstraintLayout style="@style/constraintOfCardViewStyle">

        <androidx.cardview.widget.CardView
            android:id="@+id/not_watched_fragment_recycler_item_image_view_card_view"
            style="@style/imageCardViewCardViewNotWatchedStyle"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/not_watched_card_image"
                style="@style/imageCardViewNotWatchedStyle"
                tools:ignore="ContentDescription" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/not_watched_card_title"
            style="@style/titleCardViewNotWatchedStyle"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/item_data"
            style="@style/dataCardViewNotWatchedStyle"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/unwanted_btn"
            style="@style/unwantedButton"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/wanted_btn"
            style="@style/wantedButton"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>